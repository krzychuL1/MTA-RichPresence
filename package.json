{
  "name": "mta_richpresence",
  "productName": "MTA RichPresence",
  "version": "1.0.1",
  "description": "tak",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/krzychuL1/MTA-RichPresence.git"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "MTA RichPresence",
    "win": {
