  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^24.1.3",
    "electron-builder": "^24.3.0"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "electron-builder-squirrel-windows": "^24.0.0-alpha.8",
    "electron-squirrel-startup": "^1.0.0",
    "ps-node": "^0.1.6",
    "electron-updater": "^5.3.0"
  }
}
